{
  "name": "ezequielito",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "generate": "graphql-codegen --config codegen.yml && copyfiles -u 1 src/**/*.graphql dist",
    "start": "node dist/index.js",
    "dev": "NODE_ENV=local ts-node-dev --respawn src/index.ts",
    "tsc": "tsc",
    "test": "jest --verbose --coverage",
    "test-ci": "yarn tsc && yarn test",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "build": "yarn tsc",
    "deploy": "git push heroku main"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^2.6.2",
    "@graphql-codegen/schema-ast": "^2.4.0",
    "@graphql-codegen/typescript": "^2.4.8",
    "@graphql-codegen/typescript-resolvers": "^2.6.1",
    "@types/jest": "^27.4.1",
    "@types/lodash": "^4.14.178",
    "@types/node": "^17.0.25",
    "@typescript-eslint/eslint-plugin": "^5.6.0",
    "@typescript-eslint/parser": "^5.6.0",
    "copyfiles": "^2.4.1",
    "eslint": "^8.4.1",
    "jest": "^27.5.1",
    "pino-pretty": "^7.6.0",
    "prettier": "^2.5.1",
    "ts-jest": "^27.1.4",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.6.3"
  },
  "dependencies": {
    "@graphql-tools/graphql-file-loader": "^7.3.3",
    "@graphql-tools/load": "^7.5.1",
    "@graphql-tools/schema": "^8.3.1",
    "apollo-server-express": "^3.5.0",
    "axios": "^0.26.1",
    "date-fns": "^2.28.0",
    "dotenv": "^16.0.0",
    "express": "^4.17.1",
    "graphql": "^16.3.0",
    "helmet": "^5.0.2",
    "lodash": "^4.17.21",
    "mongoose": "^6.2.10",
    "pino": "^7.5.1",
    "prisma": "^3.12.0",
    "reflect-metadata": "^0.1.13"
  }
}
