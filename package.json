  "scripts": {
    "build": "yarn generate && yarn tsc",
    "check-types": "tsc -p tsconfig.json --noemit",
    "deploy:db": "ts-node ./scripts/deploy-db",
    "dev": "ts-node-dev --respawn --transpile-only --exit-child --watch src src/index.ts",
    "dev:backup": "PORT=4001 ts-node-dev --respawn --transpile-only --exit-child --watch src src/index.ts",
    "dev:debug": "NODE_OPTIONS=\"--trace-warnings\" ts-node-dev --inspect --respawn --transpile-only --exit-child --watch src src/index.ts",
    "dev:prod": "ts-node --transpile-only src src/index.ts",
    "generate": "graphql-codegen --config codegen.yml && copyfiles -u 1 src/**/*.graphql dist",
    "lint": "eslint . && yarn check-types",
    "lint:fix": "eslint . --fix",
    "postinstall": "patch-package",
    "prepare": "husky",
    "prisma:generate": "yarn prisma generate",
    "start": "node dist/index.js",
    "start:server": "pm2 start npm --name \"DarumaApi\" -- start",
    "task": "ts-node ./src/tasks/command.ts",
    "test": "jest --verbose --detectOpenHandles",
    "test:coverage": "jest --verbose --detectOpenHandles --collectCoverage",
    "tsc": "tsc"
  },
  "version": "4.12.13"
